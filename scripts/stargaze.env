export CLI="starsd"
export CLI_OUTPUT="json"
export CLI_DENOM="ustars"
# max gas: -1, `starsd q params subspace baseapp BlockParams`, not yet implemented: https://stargaze-testnet-api.polkachu.com/cosmos/consensus/v1/params
